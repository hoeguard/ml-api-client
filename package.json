		"@adonisjs/framework": "^4.0.27",
		"@adonisjs/ignitor": "^2.0.0",
		"@adonisjs/lucid": "^4.0.22",
		"@adonisjs/session": "^1.0.2",
		"@adonisjs/shield": "^1.0.8",
		"@adonisjs/websocket": "^1.0.11",
